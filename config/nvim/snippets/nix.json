{
  "fetchFromGitHub": {
    "prefix": "ffgh",
    "body": [
      "pkgs.fetchFromGitHub {",
      "\towner = \"${1:owner}\";",
      "\trepo = \"${2:repo}\";",
      "\trev = \"${3:rev}\";",
      "\thash = \"${4:hash}\";",
      "};"
    ],
    "description": "add a fetch from GitHub in nix"
  }
}
